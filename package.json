    "localstorage-polyfill": "^1.0.1",
    "moment": "^2.29.4",
    "next": "12.3.1",
    "next-images": "^1.8.4",
    "next-themes": "^0.2.1",
    "react": "18.2.0",
    "react-ace": "7.0.4",
    "react-dom": "18.2.0",
    "react-icons": "^4.6.0",
    "react-toastify": "^9.0.8"
  },
